{
  "name": "expensetrackerappfinal",
  "version": "1.0.0",
  "description": "Expense Tracker Monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd apps/frontend && pnpm dev",
    "dev:backend": "cd apps/backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd apps/frontend && pnpm build",
    "build:backend": "cd apps/backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd apps/frontend && pnpm test",
    "test:backend": "cd apps/backend && npm test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd apps/frontend && pnpm lint",
    "lint:backend": "cd apps/backend && npm run lint",
    "format": "npm run format:frontend",
    "format:frontend": "cd apps/frontend && pnpm format"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1399.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.3",
    "nodemon": "^2.0.22",
    "razorpay": "^2.8.6",
    "sendinblue": "^0.0.1",
    "sequelize": "^6.32.0",
    "sib-api-v3-sdk": "^8.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git@github.com-personal:sudeep-h/expenseTrackerAppFinal.git"
  },
  "keywords": []
}
